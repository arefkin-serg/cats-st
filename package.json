  "svg-to-ts": {
    "conversionType": "object",
    "objectName": "svgIcon",
    "delimiter": "KEBAB",
    "srcFiles": [
      "./src/assets/svg-icons/*.svg"
    ],
    "outputDirectory": "./src/components/SvgIcons/auto-generated-icons/",
    "fileName": "svg-icons",
    "svgoConfig": {
      "plugins": [
        {
          "removeDimensions": true,
          "cleanupAttrs": true
        }
      ]
    }
